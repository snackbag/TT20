	"id": "${id}",
	"version": "${modversion}",
	"name": "${display_name}",
	"description": "${description}",
	"authors": [
		"${author}"
	],
	"contact": {
		"email": "contact@snackbag.net",
		"homepage": "https://modrinth.com/mod/TT20/",
